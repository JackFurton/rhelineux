object RetScraper {
  def main(args: Array[String]): Unit = {
    val driver: WebDriver = new FirefoxDriver()
    driver.get("https://twitter.com/AoretSC2")
    val tweets = driver.findElements(By.cssSelector(".tweet-text"))  // You'll need to update the selector based on Twitter's current layout.
